{
  "name": "jamiatulama-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.9.11",
    "@material-ui/icons": "^4.9.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.36",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-router-dom": "^5.1.4",
    "axios": "^0.19.2",
    "firebase": "^7.15.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-phone-input-2": "^2.13.7",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "typescript": "^3.7.5"
  },
  "scripts": {
    "build:scss": "node-sass -r --output-style compressed src/scss/index.scss -o src/css",
    "watch:scss": "npm run build:scss && npm run build:scss -- -w -r",
    "start:ts": "react-scripts start",
    "start": "set PORT=80 && npm-run-all -p watch:scss start:ts",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start:lambda": "netlify-lambda serve src/lambda",
    "build": "run-p build:**",
    "build:app": "react-scripts build",
    "build:lambda": "netlify-lambda build src/lambda"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/node-sass": "^4.11.0",
    "gh-pages": "^2.2.0",
    "netlify-lambda": "^2.0.1",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5"
  }
}
